<ul id="post-tags" class="breadcrumb">
    <% if @tags: %>
        <% for tag in @tags: %>
            <li>
              <span class="glyphicon glyphicon-tag"></span>
              <span class="spTag"> <a href="<%- @getTagUrl(tag) %>"><%- tag %></a></span>
            </li>
        <% end %>
    <% end %>
</ul>